[DEFAULT]
module = pyFilmQAmod
language = python
version = 3.x
configpath = ./config/
; At the Hospital
;exportpath = /home/radiofisica/Shares/Radiofisica/Medidas Pacientes/IMRT/
; At home (iMac)
;exportpath = /Users/maqui/Documents/devel/stFilmQAp/patTests/
; If using the MacBook
;exportpath = /Users/cesar/devel/GitHub/stFilmQAp/patTests/
exportpath=/Users/cesar/devel/GitHub/stFilmQAp/patTests/

[Scanner]
brand = Microtek
model = 1000XL
format = TIFF
mode = Transmission
dpi = 72
coding = RGB
bitdepth = 16

[DosePlane]
dmax = 12
pixelsize = 1
pixelunit = mm
pixelnumber = 160x160
sizeunit = mm
width = 160
height = 160

[Segmentation]
file = bb.csv

[Base]
margin = 5
marginunit = pixel

[Models]
file = RacMPhMCalibs.xlsx
mphsheet = MultiFase
racsheet = Racionales
oadcfile = Microtek1000XLOADc.npy

[Calibration]
mode = PDD
dmax = 6
energy = 6
eunit = MV
unit = cm
fieldwidth = 10
fieldheigh = 10
um = 346.5
dfs = 75
path = ./config/
file = PDDCalib.xlsx
sheet = Hoja1
shift = 1
depthmin = 2
depthmax = 17.5

[NonLocalMeans]
patchsize = 3
patchdistance = 10
h = 0.05
multichannel = True
channelaxis = 2
wred = 10
wgreen = 5
wblue = 1

